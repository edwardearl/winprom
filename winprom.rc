IDR_GRAPHTYPE MENU DISCARDABLE 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New\tCtrl+N",                ID_FILE_NEW
        POPUP "&Open"
        BEGIN
            MENUITEM "&Elevation map...\tShift+O",  ID_OPEN_ELEV_MAP
            MENUITEM "Domain &map...",              ID_OPEN_DLPR_MAP
            MENUITEM "Divide &tree...\tO",          ID_OPEN_DLPR_TREE
            MENUITEM "&Database...",                ID_OPEN_DATABASE
            MENUITEM "Mar&k map...",                ID_OPEN_MARK_MAP
        END
        POPUP "&Merge"
        BEGIN
            MENUITEM "&Elevation map...\tShift+M",  ID_MERGE_ELEV_MAP
            MENUITEM "Domain &map...",              ID_MERGE_DLPR_MAP
            MENUITEM "Divide &tree...\tM",          ID_MERGE_DLPR_TREE
            MENUITEM "&Database...",                ID_MERGE_DATABASE
        END
        POPUP "&Save"
        BEGIN
            MENUITEM "&Elevation map\tShift+S",     ID_SAVE_ELEV_MAP
            MENUITEM "Domain &map",                 ID_SAVE_DLPR_MAP
            MENUITEM "Divide &tree\tS",             ID_SAVE_DLPR_TREE
            MENUITEM "&Database",                   ID_SAVE_DATABASE
            MENUITEM "&All\tAlt+S",                 ID_SAVE_ALL
        END
        POPUP "Save &as"
        BEGIN
            MENUITEM "&Elevation map...\tShift+A",  ID_SAVEAS_ELEV_MAP
            MENUITEM "Domain &map...",              ID_SAVEAS_DLPR_MAP
            MENUITEM "Divide &tree...\tA",          ID_SAVEAS_DLPR_TREE
            MENUITEM "&Database...",                ID_SAVEAS_DATABASE
            MENUITEM "Mar&k map...",                ID_SAVEAS_MARK_MAP
        END
        POPUP "&Information"
        BEGIN
            MENUITEM "&Elevation map...\tShift+I",  ID_INFO_ELEV_MAP
            MENUITEM "Domain &map...",              ID_INFO_DLPR_MAP
            MENUITEM "Divide &tree...\tI",          ID_INFO_DLPR_TREE
            MENUITEM "&Database...",                ID_INFO_DATABASE
            MENUITEM "&All...\tAlt+I",              ID_INFO_ALL
        END
        POPUP "&Close"
        BEGIN
            MENUITEM "&Elevation map\tShift+C",     ID_CLOSE_ELEV_MAP
            MENUITEM "Domain &map",                 ID_CLOSE_DLPR_MAP
            MENUITEM "Divide &tree\tC",             ID_CLOSE_DLPR_TREE
            MENUITEM "&Database",                   ID_CLOSE_DATABASE
            MENUITEM "&All contents\tAlt+C",        ID_CLOSE_ALL
            MENUITEM "Entire &workspace",           ID_FILE_CLOSE
        END
        MENUITEM SEPARATOR
        MENUITEM "&Print...\tCtrl+P",           ID_FILE_PRINT
        MENUITEM "Print Pre&view",              ID_FILE_PRINT_PREVIEW
        MENUITEM "P&rint Setup...",             ID_FILE_PRINT_SETUP
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tAlt+X",                ID_APP_EXIT
    END
    POPUP "&Edit"
    BEGIN
        POPUP "&Copy"
        BEGIN
            MENUITEM "&Features\tCtrl+C",           ID_EDIT_COPY
            MENUITEM "&Elevation map",              ID_EDIT_COPY_ELEV_MAP
            MENUITEM "Domain &map",                 ID_EDIT_COPY_DLPR_MAP
            MENUITEM "Divide &tree",                ID_EDIT_COPY_DLPR_TREE
            MENUITEM "&Database",                   ID_EDIT_COPY_DATABASE
            MENUITEM "&Workspace",                  ID_EDIT_COPY_ALL
        END
        POPUP "&Paste"
        BEGIN
            MENUITEM "&Features\tCtrl+V",           ID_EDIT_PASTE
            MENUITEM "&Elevation map",              ID_EDIT_PASTE_ELEV_MAP
            MENUITEM "Domain &map",                 ID_EDIT_PASTE_DLPR_MAP
            MENUITEM "Divide &tree",                ID_EDIT_PASTE_DLPR_TREE
            MENUITEM "&Database",                   ID_EDIT_PASTE_DATABASE
            MENUITEM "&Workspace",                  ID_EDIT_PASTE_ALL
        END
        MENUITEM "&Unview\t<Del>",              ID_EDIT_UNVIEW
        POPUP "S&elect"
        BEGIN
            MENUITEM "&All\tCtrl+A",                ID_EDIT_SELECT_ALL
            MENUITEM "&Invert\tCtrl+I",             ID_EDIT_SELECT_INVERT
            POPUP "&Filter"
            BEGIN
                MENUITEM "&Set",                        ID_EDIT_SELECT_FILTER_SET

                MENUITEM "&Clear",                      ID_EDIT_SELECT_FILTER_CLEAR

                MENUITEM "&Precise",                    ID_EDIT_SELECT_FILTER_PRECISE

            END
        END
        MENUITEM "Searc&h\tF3",                 ID_EDIT_SEARCH
        MENUITEM "Ce&nter\tNum-5",              ID_EDIT_CENTER
        MENUITEM "&Browse",                     ID_EDIT_BROWSE
        MENUITEM "R&QF information...",         ID_EDIT_RQF
        MENUITEM SEPARATOR
        MENUITEM "&Feature...\t<CR>",           ID_EDIT_FEATURE
        MENUITEM "&Splice",                     ID_EDIT_SPLICE
        MENUITEM "&Rotate",                     ID_EDIT_ROTATE
        MENUITEM "&Zero probability",           ID_ZERO_PROB
        MENUITEM "S&addle status",              ID_SADDLE_STAT
        MENUITEM "S&WI switch",                 ID_EDIT_SWI
        MENUITEM "&Delete",                     ID_EDIT_DELETE
        MENUITEM "&Transfer info...",           ID_EDIT_TRANSFER
    END
    POPUP "&View"
    BEGIN
        POPUP "&Zoom"
        BEGIN
            MENUITEM "&Way in",                     ID_VIEW_ZOOM_WAYIN
            MENUITEM "&In\t+",                      ID_VIEW_ZOOM_IN
            MENUITEM "&Out\t-",                     ID_VIEW_ZOOM_OUT
            MENUITEM "W&ay out",                    ID_VIEW_ZOOM_WAYOUT
            MENUITEM "&Set scale...",               ID_VIEW_ZOOM_SCALE
        END
        MENUITEM "Scroll &center\t@",           ID_VIEW_SCROLL_CENTER
        POPUP "&Ridges"
        BEGIN
            MENUITEM "&Peak - to - peak\tShift+F9", ID_VIEW_RIDGE_PEAK
            MENUITEM "Peak - &saddle - peak\tShift+F10", 
                                                    ID_VIEW_RIDGE_SADDLE
            MENUITEM "Peak - &runoff\tShift+F11",   ID_VIEW_RIDGE_RUNOFF
            MENUITEM "Peak - &basin saddle\tShift+F12", 
                                                    ID_VIEW_RIDGE_BASINSADL
            MENUITEM "&All\tAlt+R",                 ID_VIEW_RIDGE_ALL
            MENUITEM "&None\tCtrl+Alt+R",           ID_VIEW_RIDGE_NONE
            MENUITEM "&Options...",                 ID_VIEW_RIDGE_OPTIONS
        END
        POPUP "F&eatures"
        BEGIN
            MENUITEM "&Peaks\tF9",                  ID_VIEW_FEATURES_PEAKS
            MENUITEM "&Saddles\tF10",               ID_VIEW_FEATURES_SADDLES
            MENUITEM "&Runoffs\tF11",               ID_VIEW_FEATURES_RUNOFFS
