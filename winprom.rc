        MENUITEM "New &profile",                ID_WINDOW_PROFILE
        MENUITEM SEPARATOR
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&File bar",                   ID_VIEW_TB_DATABASE
        MENUITEM "&Status bar",                 ID_VIEW_STATUS_BAR
    END
    POPUP "&Help", HELP
    BEGIN
        MENUITEM "&Contents",                   ID_HELP_FINDER
        MENUITEM "&Help",                       ID_HELP_INDEX
        MENUITEM "&Quick start\tAlt+F1",        ID_HELP_QUICK
        MENUITEM "&Window\tF1",                 ID_HELP
        MENUITEM "Conte&xt\tShift+F1",          ID_CONTEXT_HELP
        MENUITEM SEPARATOR
        MENUITEM "&About...",                   ID_APP_ABOUT
    END
END


