                MENUITEM "&Set",                        ID_EDIT_SELECT_FILTER_SET

                MENUITEM "&Clear",                      ID_EDIT_SELECT_FILTER_CLEAR

                MENUITEM "&Precise",                    ID_EDIT_SELECT_FILTER_PRECISE

            END
        END
        MENUITEM "Searc&h\tF3",                 ID_EDIT_SEARCH
        MENUITEM "C&enter\tNum-5",              ID_EDIT_CENTER
        MENUITEM "&Browse",                     ID_EDIT_BROWSE
        MENUITEM "R&QF information...",         ID_EDIT_RQF
        MENUITEM SEPARATOR
        MENUITEM "&Data...\t<CR>",              ID_EDIT_RECORD
        MENUITEM "Delete",                      ID_EDIT_DELETE
    END
    POPUP "&List"
    BEGIN
        MENUITEM "&Format...\tCtrl+F",          ID_LIST_FORMAT
        MENUITEM "&Relist...\tCtrl+R",          ID_LIST_RELIST
        MENUITEM "&Summary...\tCtrl+S",         ID_LIST_SUMMARY
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&List",                       ID_VIEW_LISTX
        MENUITEM "&Report",                     ID_VIEW_REPORT
        MENUITEM SEPARATOR
